{
	"name": "Go AWS lambda",
	"dockerComposeFile": "../docker-compose.yml",
	"workspaceFolder": "/app",
	"service": "app", 
	"customizations": {
		"vscode": {
			"settings": {
				"go.gopath": "/go"
			},
			"extensions": [
				"golang.go",
				"ms-azuretools.vscode-docker",
				"humao.rest-client"
			]
		}
	},
	"forwardPorts": [8000]
}

